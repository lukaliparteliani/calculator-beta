l":{"line":268,"column":0},"generated":{"line":268,"column":0}},{"source":"stylesheet.css","original":{"line":269,"column":0},"generated":{"line":269,"column":0}},{"source":"stylesheet.css","original":{"line":270,"column":0},"generated":{"line":270,"column":0}},{"source":"stylesheet.css","original":{"line":271,"column":0},"generated":{"line":271,"column":0}},{"source":"stylesheet.css","original":{"line":272,"column":0},"generated":{"line":272,"column":0}},{"source":"stylesheet.css","original":{"line":273,"column":0},"generated":{"line":273,"column":0}},{"source":"stylesheet.css","original":{"line":274,"column":0},"generated":{"line":274,"column":0}},{"source":"stylesheet.css","original":{"line":275,"column":0},"generated":{"line":275,"column":0}},{"source":"stylesheet.css","original":{"line":276,"column":0},"generated":{"line":276,"column":0}},{"source":"stylesheet.css","original":{"line":277,"column":0},"generated":{"line":277,"column":0}}],"sources":{"stylesheet.css":"* {\n  -webkit-box-sizing: border-box;\n  -moz-box-sizing: border-box;\n  box-sizing: border-box;\n}\n\nbody {\n  padding: 0;\n  position: relative;\n  background-color: rgb(59, 74, 56);\n}\n\n.calc {\n    width:100%;\n    position: relative;\n    display: flex;\n    align-items: center;\n    flex-direction: column;\n    height: auto;\n    min-height: 600px;\n    max-width: 600px;\n    padding: 1rem 1rem;\n    z-index: 1;\n    margin-left: auto;\n    margin-right: auto;\n}\n\n.screen {\n    height: 50px;\n    border-radius: 20px;  \n    overflow: hidden;\n    background-color: white;\n    margin: 40px 40px 0 40px;\n    display: flex;\n    justify-content: flex-end;\n    align-items: center;\n    font-size: 40px;\n    padding-right: 20px;\n    text-transform: uppercase;\n    font-weight: bold;\n    align-self: stretch;\n    transition: all 400ms ease-in;\n}\n\n  .calc-buttons {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n  }\n\n  .calc-row {\n    display: flex;\n    gap: 10px;\n    margin: 10px;\n  }\n\n  .calc-row:first-child {\n    margin: 30px 0;\n    column-gap: 10px;\n    row-gap: 20px;\n    flex-wrap: wrap;\n    align-items: center;\n    justify-content: center;\n  }\n  \n  .calc-button:hover {background-color: #c9c9c9}\n  \n  .calc-button:active {\n    background-color: #c9c9c9;\n    box-shadow: 0 5px #666;\n    transform: translateY(4px);\n  }\n\n  .calc-button {\n    border-radius: 100%;\n    background-color: beige;\n    width: 70px;\n    height: 70px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    font-size: 40px;\n    border: none;\n    cursor: pointer;\n    outline: none;\n    box-shadow: 0 9px #98C;\n    transition: all 100ms ease-in;\n    font-weight: bold;\n  }\n\n  .calc-row:first-child .calc-button {\n    background-color: rgb(240, 196, 76);\n    box-shadow: 0 9px rgb(185, 180, 201);\n  }\n  \n\n  .calc-row:first-child .calc-button:hover, .calc-row:last-child .calc-button:hover {background-color: #e7e7e7}\n  \n  .calc-row:first-child .calc-button:active,  .calc-row:last-child .calc-button:active {\n    background-color: #a3a3a3;\n    box-shadow: 0 5px #666;\n  }\n\n  .calc-row:last-child {\n    flex-direction: column;\n    position: absolute;\n    gap: 20px;\n    top: 126px;\n    right: 28px;\n  }\n\n  .calc-row:last-child .calc-button {\n    width: 80px;\n    height: 80px;\n    color:ivory;\n    background-color: rgb(225, 143, 176);\n    box-shadow: 0 9px rgb(185, 180, 201);\n  }\n\n  @media screen and (max-width: 570px) {\n    .calc {\n      align-items: flex-start;\n    }\n\n    .circles {\n      display: none;\n    }\n  }\n\n  @media screen and (max-width: 470px) {\n    .calc {\n      align-items: flex-start;\n    }\n\n    .calc-buttons {\n      align-items: flex-start;\n    }\n\n    .calc-row:last-child {\n      top: 212px;\n      right: 0px;\n    }\n  }\n\n  @media screen and (max-width: 400px) {\n    .calc-button {\n      width: 50px;\n      height: 50px;\n      font-size: 30px;\n      box-shadow: 0 4px rgb(185, 180, 201);\n    }\n\n    .calc-row:first-child .calc-button {\n      box-shadow: 0 4px rgb(185, 180, 201);\n    }\n\n    .calc-row:last-child .calc-button {\n      width: 60px;\n      height: 60px;\n      box-shadow: 0 6px rgb(185, 180, 201);\n    }\n  }\n\n.circles{\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    overflow: hidden;\n}\n\n.circles li{\n    position: absolute;\n    display: block;\n    list-style: none;\n    width: 20px;\n    height: 20px;\n    background: rgba(255, 255, 255, 0.2);\n    animation: animate 25s linear infinite;\n    bottom: -150px;\n}\n\n.circles li:nth-child(1){\n    left: 25%;\n    width: 80px;\n    height: 80px;\n    animation-delay: 0s;\n}\n\n\n.circles li:nth-child(2){\n    left: 10%;\n    width: 20px;\n    height: 20px;\n    animation-delay: 2s;\n    animation-duration: 12s;\n}\n\n.circles li:nth-child(3){\n    left: 70%;\n    width: 20px;\n    height: 20px;\n    animation-delay: 4s;\n}\n\n.circles li:nth-child(4){\n    left: 40%;\n    width: 60px;\n    height: 60px;\n    animation-delay: 0s;\n    animation-duration: 18s;\n}\n\n.circles li:nth-child(5){\n    left: 65%;\n    width: 20px;\n    height: 20px;\n    animation-delay: 0s;\n}\n\n.circles li:nth-child(6){\n    left: 75%;\n    width: 110px;\n    height: 110px;\n    animation-delay: 3s;\n}\n\n.circles li:nth-child(7){\n    left: 35%;\n    width: 150px;\n    height: 150px;\n    animation-delay: 7s;\n}\n\n.circles li:nth-child(8){\n    left: 50%;\n    width: 25px;\n    height: 25px;\n    animation-delay: 15s;\n    animation-duration: 45s;\n}\n\n.circles li:nth-child(9){\n    left: 20%;\n    width: 15px;\n    height: 15px;\n    animation-delay: 2s;\n    animation-duration: 35s;\n}\n\n.circles li:nth-child(10){\n    left: 85%;\n    width: 150px;\n    height: 150px;\n    animation-delay: 0s;\n    animation-duration: 11s;\n}\n\n\n\n@keyframes animate {\n\n    0%{\n        transform: translateY(0) rotate(0deg);\n        opacity: 1;\n        border-radius: 0;\n    }\n\n    100%{\n        transform: translateY(-1000px) rotate(720deg);\n        opacity: 0;\n        border-radius: 50%;\n    }\n\n}\n"},"lineCount":277}},"error":null,"hash":"80f545b5e1d0c10217338830f6b292c9","cacheData":{"env":{}}}